	file = {PDF:/Users/janoleschmidt/Zotero/storage/BDHNQUJM/Stalnaker et al. - 2025 - Developer Perspectives on Licensing and Copyright Issues Arising from Generative AI for Software Dev.pdf:application/pdf},
}

@inproceedings{khan_beyond_2025,
	title = {Beyond Automation: How {UI}/{UX} Designers Perceive {AI} as a Creative Partner in the Divergent Thinking Stages},
